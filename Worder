def copy(s):
    news=[]
    for i in s:
        news.append(i)
    return news

def split_into_words(s):
    sentences=""
    words=[]
    word=""
    ignorespace=False
    for letter in s:
        if letter==' ' and ignorespace==False:
            words.append(word)
            word=""
        elif letter==' ' and ignorespace==True:
            ignorespace=False
        elif letter==".":
            words.append(word)
            sentences.append(copy(words))
            words=[]
            word=""
            ignorespace=True
        else:
            word+=letter
    return words
def alph_index(letter):
    if ord(letter)>64 and ord_letter<91:
        index=ord(letter)-64
    elif ord(letter)>96 and ord_letter<123:
        index=ord(letter)-96
    return index

